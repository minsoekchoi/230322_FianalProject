3. Boardlist
 - Table 명 : post
 - 관련 Query문
 
 1)
CREATE TABLE POST
(
POST_IDX NUMBER PRIMARY KEY,
POST_TITLE VARCHAR2(100) NOT NULL,
POST_CONTENT VARCHAR2(200) NOT NULL,
POST_DATETIME DATE NOT NULL,
POST_HIT NUMBER DEFAULT 0,
U_IDX NUMBER NOT NULL,
POST_CODE NUMBER DEFAULT 0,
POST_LIKE NUMBER DEFAULT 0,
POST_IMG VARCHAR2(50),
ADMIN_IDX NUMBER NOT NULL,
POST_DEL NUMBER DEFAULT 0,
POST_DEL_DATETIME DATE
);

 2)
CREATE SEQUENCE POST_SEQ
INCREMENT BY 1
START WITH 1
MINVALUE 1
MAXVALUE 9999
CYCLE
NOCACHE
NOORDER;

 3)
INSERT INTO POST
(POST_IDX, POST_TITLE, POST_CONTENT, POST_DATETIME, U_IDX, POST_IMG, ADMIN_IDX, POST_DEL_DATETIME)
VALUES (USERS_SEQ.NEXTVAL
, '테스트0001'
, '테스트 내용입니다.'
, SYSDATE
, 1
, 'test01'
, 99
, SYSDATE
);

4. Boardlist_room
 - Table 명 : room
 - 관련 Query문
 
 1)
CREATE TABLE room
(
ROOM_IDX NUMBER PRIMARY KEY,
ROOM_TITLE VARCHAR2(100) NOT NULL,
CATEGORY_TYPE NUMBER NOT NULL,
ROOM_COUNT NUMBER NOT NULL,
ROOM_REGION VARCHAR2(50)NOT NULL,
ROOM_GENDER NUMBER NOT NULL,
START_DATETIME DATE NOT NULL,
END_DATETIME DATE NOT NULL,
ROOM_CONTENT VARCHAR2(250) NOT NULL,
WRITE_DATETIME DATE NOT NULL,
CLOSE_OK NUMBER NOT NULL,
U_NICKNAME VARCHAR2(50) NOT NULL,
U_IDX NUMBER NOT NULL,
BEGIN_DATETIME DATE NOT NULL,
FINAL_DATETIME DATE NOT NULL,
ROOM_DEL_CODE NUMBER DEFAULT 0,
ADMIN_IDX NUMBER,
ROOM_DEL_ADMIN VARCHAR2(50),
ROOM_DEL_DATETIME DATE
);

 2)
CREATE SEQUENCE ROOM_SEQ
INCREMENT BY 1
START WITH 1
MINVALUE 1
MAXVALUE 9999
CYCLE
NOCACHE
NOORDER;

 3)
INSERT INTO room
(ROOM_IDX,ROOM_TITLE, CATEGORY_TYPE, ROOM_COUNT, ROOM_REGION, ROOM_GENDER, START_DATETIME, END_DATETIME, ROOM_CONTENT, WRITE_DATETIME, CLOSE_OK, U_NICKNAME, U_IDX, BEGIN_DATETIME, FINAL_DATETIME  )
VALUES (ROOM_SEQ.NEXTVAL
, '테스트방 0001'
, 0
, 4
, '마포구'
, 3
, SYSDATE
, SYSDATE
, '테스트입니다.'
, SYSDATE
, 0
, '테스터0001'
, 1
, SYSDATE
, SYSDATE
);

5. Adminlist_Super
 - Table 명 : ADMIN
 - 관련 Query문
 
 1)
CREATE TABLE ADMIN
(
ADMIN_IDX NUMBER PRIMARY KEY,
ADMIN_ID VARCHAR2(50) NOT NULL,
ADMIN_PWD VARCHAR2(50) NOT NULL,
ADMIN_SALT VARCHAR2(50) NOT NULL,
ADMIN_NICKNAME VARCHAR2(50) NOT NULL,
ADMIN_ROLE NUMBER DEFAULT 1,
ADMIN_DATETIME DATE
);

 2)
CREATE SEQUENCE ADMIN_SEQ
INCREMENT BY 1
START WITH 1
MINVALUE 1
MAXVALUE 9999
CYCLE
NOCACHE
NOORDER;

 3)
INSERT INTO ADMIN(ADMIN_IDX, ADMIN_ID, ADMIN_PWD, ADMIN_SALT, ADMIN_NICKNAME, ADMIN_DATETIME)
VALUES (ADMIN_SEQ.NEXTVAL
, 'superadmin@ict.com'
, '1234'
, 'salt'
, '최고관리자01'
, SYSDATE
);

